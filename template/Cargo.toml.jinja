[package]
name = "{{ project_name }}"
version = "0.1.0"
edition = "2024"

[dependencies]
indoc = "=2.0.6"
thiserror = "=2.0.18"

[dev-dependencies]
rstest = "=0.26.1"

[lints.clippy]
allow_attributes = "warn"
allow_attributes_without_reason = "warn"
disallowed_macros = "deny"
expect_used = "deny"
panic = "deny"
unwrap_used = "deny"
